{
  "name": "react-todo",
  "author": "lazysergey",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "next": "^9.0.6",
    "node-sass": "^7.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rxjs": "^6.5.3",
    "concurrently": "^4.1.2",
    "json-server": "^0.15.1",
    "prop-types": "^15.7.2",
    "react-scripts": "3.1.2",
    "css-loader": "1.0.1"
  },
  "scripts": {
    "start-ssr": "concurrently --kill-others 'next' 'json-server todo.database.json -p=3333 -d=0'",
    "start-ssrd": "concurrently --kill-others 'next' 'json-server todo.database.json -p=3333 -d=1100'",
    "start-csr": "concurrently --kill-others 'react-scripts start' 'json-server todo.database.json -p=3333'",
    "start-csr-front": "react-scripts start",
    "start-back": "json-server todo.database.json -p=3333",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "next-dev": "next",
    "next-build": "next build",
    "next-start": "next start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
